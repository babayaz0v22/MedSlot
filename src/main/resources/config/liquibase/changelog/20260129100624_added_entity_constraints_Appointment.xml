<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd
                        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">
    <!--
        Added the constraints for entity Appointment.
    -->
    <changeSet id="20260129100624-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="clinic_id"
                                 baseTableName="main_appointment"
                                 constraintName="fk_main_appointment__clinic_id"
                                 referencedColumnNames="id"
                                 referencedTableName="main_clinic"
                                 />

        <addForeignKeyConstraint baseColumnNames="doctor_id"
                                 baseTableName="main_appointment"
                                 constraintName="fk_main_appointment__doctor_id"
                                 referencedColumnNames="id"
                                 referencedTableName="main_doctor"
                                 />

        <addForeignKeyConstraint baseColumnNames="patient_id"
                                 baseTableName="main_appointment"
                                 constraintName="fk_main_appointment__patient_id"
                                 referencedColumnNames="id"
                                 referencedTableName="main_patient"
                                 />
    </changeSet>
</databaseChangeLog>
